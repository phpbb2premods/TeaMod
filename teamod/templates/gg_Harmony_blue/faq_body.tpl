</table>

<!-- BEGIN faq_block -->
<table class="forumline" width="100%" align="center" cellpadding="3" cellspacing="1" border="0">
	<tr>
		<td class="catHead" height="28" align="center"><span class="cattitle">{faq_block.BLOCK_TITLE}</span></td>
	</tr>
	<!-- BEGIN faq_row -->  
	<tr>
		<td class="{faq_block.faq_row.ROW_CLASS}" align="left" valign="top"><span class="postbody"><a class="postlink" href="#top"><img src="{I_UP_ARROW}" width="9" height="9" alt="{L_BACK_TO_TOP}" title="{L_BACK_TO_TOP}" border="0" /></a>&nbsp;<a name="{faq_block.faq_row.U_FAQ_ID}"></a><b>{faq_block.faq_row.FAQ_QUESTION}</b></span><br /><span class="postbody">{faq_block.faq_row.FAQ_ANSWER}</span></td>
	</tr>
	<tr>
		<td class="spaceRow" width="100%" height="1"><img src="{I_SPACER}" alt="" width="1" height="1" /></td>
	</tr>
	<!-- END faq_row -->
</table>
