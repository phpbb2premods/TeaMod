<table cellspacing="1" cellpadding="2" border="0" width="100%" class="forumline">
<tr>
	<td class="row1" nowrap="nowrap">
		<span class="topictitle">
			<!-- BEGIN msg_icon --><img src="{msg_icon.I_ICON}" border="0" title="{msg_icon.L_ICON}" alt="{msg_icon.L_ICON}" />&nbsp;<!-- END msg_icon -->
			<!-- BEGIN sub_type --><!-- BEGIN img --><img src="{sub_type.I_SUB_TYPE}" border="0" alt="{sub_type.L_SUB_TYPE}" title="{sub_type.L_SUB_TYPE}" class="absbottom" /><!-- END img --><!-- BEGIN txt --><b>[{sub_type.L_SUB_TYPE}]</b><!-- END txt -->&nbsp;<!-- END sub_type -->
			<a href="{U_TOPIC}" class="topictitle">{TOPIC_TITLE}</a>
		</span><span class="gensmall">
			<!-- BEGIN sub_title --><br />{sub_title.SUB_TITLE}<!-- END sub_title -->
			<!-- BEGIN calendar_event --><br />{calendar_event.S_CALENDAR_EVENT}<!-- END calendar_event -->
			<!-- BEGIN text --></span><hr /><span class="gensmall"><b>{L_AUTHOR}:</b>&nbsp;{AUTHOR}<!-- END text -->
			<br /><b>{L_TOPIC_DATE}:</b>&nbsp;{TOPIC_DATE}
			<!-- BEGIN announce -->&nbsp;- {announce.S_ANNOUNCE}<!-- END announce -->
			<br /><b>{L_FORUM}:</b>&nbsp;<!-- BEGIN nav --><a href="{nav.U_NAV}" title="{nav.L_NAV_DESC}" class="gensmall">{nav.L_NAV}</a><!-- BEGIN sep -->&nbsp;&raquo;&nbsp;<!-- END sep --><!-- END nav -->
		</span>
	</td>
</tr>
<tr>
	<td class="row1">
		<!-- BEGIN text --><span class="genmed">{MESSAGE}</span><hr /><!-- END text -->
		<span class="gensmall">
			<b>{L_VIEWS}:</b>&nbsp;{VIEWS}<br />
			<b>{L_REPLIES}:</b>&nbsp;{REPLIES}
		</span>
	</td>
</tr>
</table>