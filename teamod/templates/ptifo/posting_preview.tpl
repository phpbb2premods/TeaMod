 
<table class="forumline" width="100%" cellspacing="1" cellpadding="4" border="0">
	<tr> 
		<th height="25" class="thHead">{L_PREVIEW}</th>
	</tr>
	<tr> 
		<td class="row1">
			<!-- BEGIN title --><div class="postbody"><b><!-- END title -->
			<!-- BEGIN msg_icon --><img src="{msg_icon.I_ICON}" border="0" title="{msg_icon.L_ICON}" class="absbottom" alt="" /><!-- END msg_icon -->
			<!-- BEGIN sub_type -->
				<!-- BEGIN img --><img src="{sub_type.I_SUB_TYPE}" border="0" alt="{sub_type.L_SUB_TYPE}" title="{sub_type.L_SUB_TYPE}" align="top" /><!-- END img -->
				<!-- BEGIN txt --><b>[{sub_type.L_SUB_TYPE}]</b><!-- END txt -->
			<!-- END sub_type -->
			<!-- BEGIN title -->{POST_SUBJECT}</b>
			</div><div class="postdetails"><!-- END title -->
			<!-- BEGIN sub_title -->{sub_title.SUB_TITLE}<br /><!-- END sub_title -->
			<!-- BEGIN announce -->{announce.S_ANNOUNCE}<br /><!-- END announce -->
			<!-- BEGIN calendar_event -->{calendar_event.S_CALENDAR_EVENT}<br /><!-- END calendar_event -->
			<!-- BEGIN title --><hr /></div><!-- END title -->
			<div class="postbody">
				{MESSAGE}
				<!-- BEGIN postrow -->
					{ATTACHMENTS}
				<!-- END postrow -->
			</div><div class="postdetails" align="right">
				<br /><img src="{I_POST}" alt="{L_POST}" title="{L_POST}" border="0" hspace="2" />{L_POSTED}: {POST_DATE}
			</div>
		</td>
	</tr>
	<tr> 
		<td class="spaceRow" height="1"><img src="{I_SPACER}" width="1" height="1" alt="" /></td>
	</tr>
</table>

<br clear="all" />
